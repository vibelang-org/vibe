{
  "name": "vibe",
  "version": "1.0.0",
  "description": "AI agent orchestration language and runtime",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "vibe": "src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "test": "bun test src/",
    "test:watch": "bun test --watch src/",
    "test:integration": "bun test tests/integration/ --timeout 120000",
    "test:all": "bun test src/ tests/integration/ --timeout 120000",
    "prepublish": "bun run scripts/publish.ts",
    "publish:npm": "bun run scripts/publish.ts && cd dist/npm && npm publish --access public"
  },
  "keywords": ["ai", "agent", "orchestration", "dsl", "language"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "^1.34.0",
    "chevrotain": "^11.0.3",
    "openai": "^6.15.0",
    "typescript": "^5.9.3"
  }
}
